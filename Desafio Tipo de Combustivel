using System; 

class URI 
{

    static void Main(string[] args) 
    { 
      int alcool = 0;
      int gasolina = 0;
      int diesel = 0;
      int encerrado = 0;
      int produto;

            do
            {
                produto = int.Parse(Console.ReadLine());
                
                switch (produto)
                {
                    case 1:
                      alcool++;
                      break;
                     
                    case 2:
                      gasolina++;
                      break;
                      
                    case 3:
                      diesel++;
                      break;
                        
                    case 4:
                      Console.WriteLine("MUITO OBRIGADO");
                      Console.WriteLine($"Alcool: " + alcool);
                      Console.WriteLine($"Gasolina: " + gasolina);
                      Console.WriteLine($"Diesel: " + diesel);
                      break;
                        
                    default:
                      break;
                      continue;
                }
            }
       
          while (produto!=4);
          
    }

}
